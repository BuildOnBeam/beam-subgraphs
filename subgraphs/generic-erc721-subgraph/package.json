{
  "name": "generic-erc721-subgraph",
  "license": "MIT",
  "scripts": {
    "prepare:arbitrum": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ generic-erc721-subgraph",
    "create-local": "graph create --node http://localhost:8020/ generic-erc721-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ generic-erc721-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 generic-erc721-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.67.1",
    "@graphprotocol/graph-ts": "0.32.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0",
    "mustache": "^4.2.0"
  }
}